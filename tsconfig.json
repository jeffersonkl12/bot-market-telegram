{
  "compilerOptions": {
    "target": "es2016",

    "module": "NodeNext",

    "esModuleInterop": true,
    "moduleResolution": "NodeNext",

    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowJs": true,

    "strict": true,

    "skipLibCheck": true
  }
}
